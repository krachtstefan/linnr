{
  "version": 2,
  "name": "linnr",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/credits",
      "dest": "/index.html"
    },
    {
      "src": "/highscore",
      "dest": "/index.html"
    },
    {
      "src": "/submit-highscore",
      "dest": "/index.html"
    },
    {
      "src": "/play",
      "dest": "/index.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/fonts/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    }
  ]
}